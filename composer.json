{
  "name": "cetver/yii2-language-selector",
  "description": "Provides the configuration for the language selector for the Yii framework 2.0",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "language",
    "locale",
    "i18n",
    "internationalization",
    "url",
    "select",
    "switch",
    "change",
    "pick",
    "toggle"
  ],
  "license": "BSD-3-Clause",
  "support": {
    "source": "https://github.com/cetver/yii2-language-selector",
    "issues": "https://github.com/cetver/yii2-language-selector/issues"
  },
  "authors": [
    {
      "name": "Alexandr Cetvertacov",
      "email": "cetver@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.13"
  },
  "require-dev": {
    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1",
    "codeception/specify": "~0.4.3",
    "satooshi/php-coveralls": "^1.0"
  },
  "config": {
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "cetver\\LanguageSelector\\": ""
    }
  }
}
